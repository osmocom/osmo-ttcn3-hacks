 OCT1                   cause_Value
 } with { 
  variant "PRESENCE(iEI = '07'O)";
  variant (lengthIndicator) "LENGTHTO (cause_Value)";
  variant (lengthIndicator) "CROSSTAG( length1,    ext = '1'B;
                                       length2,    ext = '0'B;)";
