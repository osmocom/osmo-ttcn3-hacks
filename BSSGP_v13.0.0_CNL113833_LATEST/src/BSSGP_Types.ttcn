} with { 
  variant "PRESENCE(iEI = '0B'O)";
  variant (lengthIndicator) "LENGTHTO (callPriority,spare)";
  variant (lengthIndicator) "CROSSTAG( length1,    ext = '1'B;
                                       length2,    ext = '0'B;)";
  variant (ext,lengthIndicator) "FIELDORDER(msb)";
        };
        
