} with { 
  variant "PRESENCE(iEI = '05'O)";
  variant (lengthIndicator) "LENGTHTO (bmax)";
  variant (lengthIndicator) "CROSSTAG( length1,    ext = '1'B;
                                       length2,    ext = '0'B;)";
  variant (ext,lengthIndicator) "FIELDORDER(msb)";
};

